  .mejs-time-loaded,
  .mejs-time-marker,
  .mejs-time-total  { height:4px !important;}
.post_podcast .mejs-time-rail { padding-top:14px !important;}
.post_podcast .mejs-button>button { background-image: var(--wpr-bg-166fb5e5-13fc-4e85-9ed9-8ee73e15bc10) !important;}
.post_podcast .mejs-controls {
  .mejs-time-rail .mejs-time-loaded,
