}
.post_podcast .mejs-time { color:#0475F4 !important}
.post_podcast .mejs-controls .mejs-time-rail .mejs-time-current { background:#0475F4 !important;}
.post_podcast .tit { color:#0475F4; font-size:14px; font-weight:700; display:block; padding-bottom:8px;}
.post_podcast .mejs-horizontal-volume-total { height:4px !important; top:18px !important}
.post_podcast .mejs-time-handle-content {top:-6px !important;  border:2px solid #fff !important; background:#0475F4; }
.post_podcast .mejs-controls .mejs-time-rail span { border-radius:20px;}

