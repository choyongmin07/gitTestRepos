/*오늘의 퀴즈,오늘의 건강체크*/
.quiz_wrap { max-width: 700px; min-height: 100%; width: 100%; margin:20px auto 50px;}
.quiz_wrap .ConView { margin-top:-40px;}
.quiz_wrap .QuizList {padding: 5px;}
.quiz_wrap .QuizView {top: 50%; width: 100%; max-width: 1110px;}
.quiz_wrap .ConQuizView { padding: 6em 0 10em; width: 100%; font-weight: 400; }
.quiz_wrap .quiz_tit { text-align:center; border-top: none; padding:60px 20px 80px; background: var(--km-gray-sub-20); font-size: 32px !important; color: var(--km-gray-txt-100); font-weight: bold; line-height:1.2; letter-spacing: -0.02em; overflow: hidden; text-overflow: ellipsis; display: -webkit-box; -webkit-line-clamp: 3; -webkit-box-orient: vertical; word-wrap: break-word; }
.quiz_wrap .dettailBox { border: 1px solid var(--km-gray-sub-30); border-bottom: none;}
.quiz_wrap .dettailBox img { /*border-radius: 20px 20px 0 0;*/}
.quiz_wrap .introView p { font-size:20px; }
.quiz_wrap .btnwrap01 { font-size: 14px;line-height: 1.25em; margin-bottom: 30px;}
.quiz_wrap .askTit { display: table;}
.quiz_wrap .askTit { display: table-cell; vertical-align: middle; text-align: left; padding-left: 44px; padding-bottom:20px;}
.quiz_wrap em { display: block; float: left; width: 40px; height:40px; border-radius:50%; line-height: 40px; margin: 0 12px 0 -44px; color:#fff; background:var(--km-primary-40); font-size: 18px; font-style: normal; text-align:center; }
.quiz_wrap .qviewTit { font-size: 28px; vertical-align: top; line-height: 1.3em; letter-spacing: 0;}
.quiz_wrap .pr {position: relative !important;}
.quiz_wrap .reche { position:absolute !important; top: 50%; left: 50%; transform: translate(-50%, -50%); animation:recheox 0.1s cubic-bezier(0.250, 0.460, 0.450, 0.940) both;  background:var(--km-gray-sub-1);  width:200px; height:200px; border-radius:50%; text-align:center;  }
.quiz_wrap .reche img { width:70%; height:auto; margin-top:30px; }
@keyframes recheox { 0% {opacity: 0; transform: translate(-50%, -50%) scale(0); } 100% {opacity: 1; transform: translate(-50%, -50%) scale(1); } }
.quiz_wrap .qViewAskNum { display: inline-block; font-size: 12px; text-align: right; background: var(--km-gray-sub-40); padding: 1px 5px; color:var(--km-gray-txt-50); position: absolute; top:0; right: 0;}
.quiz_wrap ul.askOX { border: 1px solid var(--km-gray-sub-40); margin-top: 25px; padding:0; display:flex; }
.quiz_wrap ul.askOX li { display: inline-block; width: 50% !important; height: 240px; margin-left: 0;}
.quiz_wrap ul.askOX li a { display:block; width:100%; height:100%; }
.quiz_wrap ul.askOX li:hover { background-color:var(--km-primary-20); cursor:pointer;}
.quiz_wrap ul.askOX li a.on { background-color:var(--km-primary-20);}
:where(.anO, .anO .on, .anX, .anX .on ) {background-size: 80px; background-repeat:no-repeat; background-position:50%;  border-right: 1px solid var(--km-gray-sub-40); text-indent: -9999999px;}
.quiz_wrap .anO, li.anO a.on { background-image: var(--wpr-bg-172cf5b7-cec4-42ff-9a18-7c6e77c6a3bc) }
.quiz_wrap .anX, li.anX a.on { background-image: var(--wpr-bg-1bd6fa41-08b8-4483-a318-d8d457f8d7fe)}
.quiz_wrap .answerNow { border: 1px solid var(--km-gray-sub-40); background:var(--km-gray-sub-3); margin-top: 25px; padding: 30px; }
:where(.answerNow .anO,.answerNow .anX) { border-right:none;}
.quiz_wrap strong.result { display: block; margin-bottom: 15px; font-size: 18px; color: var(--km-gray-txt-40); text-align: center;}
.quiz_wrap .answerNow p { display: block; min-height: 90px;}
.quiz_wrap p.anO {background: var(--wpr-bg-abc5b6bc-2970-4e25-988d-e3c84924b2f0) no-repeat 50% 50%; background-size: 80px;text-indent: -9999999px; border-right: none;}
.quiz_wrap p.comment {padding: 50px 40px; font-size: 20px; line-height: 1.6; border-top: 1px solid var(--km-gray-sub-40); margin-top: 25px;}
.quiz_wrap .Btn_GB01 {background-color: var(--km-primary-40) !important;border-radius: 0px !important; font-size:18px; padding:28px 20px;}
.quiz_wrap ul.askAll {border: 1px solid var(--km-gray-sub-40);margin-top: 24px;border-bottom: none;padding-left:0;}
.quiz_wrap ul.askAll li {width: 100%;border-bottom: 1px solid var(--km-gray-sub-40);position: relative;cursor: pointer;}
.quiz_wrap ul.askAll li .inner {box-sizing: border-box;display: table;}
.quiz_wrap ul.askAll li:hover {background-color: var(--km-primary-20);}
.quiz_wrap ul.askAll li .innerOn {box-sizing: border-box;display: table;}
.quiz_wrap ul.askAll li .inner::before, .quiz_wrap ul.askAll li .innerOn::before {-webkit-background-size: 25px auto !important;background-size: 25px auto !important;display: inline-block;width: 25px;height: 25px;margin: 22px 15px 15px 15px;content: "";}
.quiz_wrap ul.askAll li .inner::before {background: var(--wpr-bg-7c6a6c26-9c5e-4c3f-8f4a-c36d227fc93d) no-repeat;}
.quiz_wrap ul.askAll li .innerOn::before {background: var(--wpr-bg-e5cf214a-c699-45b0-be1e-9b217f23b773) no-repeat;}
.quiz_wrap ul.askAll li:hover .inner::before {background: var(--wpr-bg-c1ca1307-a979-43c7-b2cd-66f8b9443e2a) no-repeat;}
.quiz_wrap ul.askAll li .inner::after {position: absolute;top: 0;right: 0;bottom: 0;left: 0;content: "";}
.quiz_wrap ul.askAll li p {display: table-cell;vertical-align: middle;font-size: 20px;padding: 20px 16px 20px 0;}
.quiz_wrap .answerNow .anTxT {min-height: 20px;font-size: 32px;line-height: 1.4;font-weight: bold;color: #D32F2F;margin: 25px 0 10px;text-align: center;}
.quiz_wrap .resultQ .scorewrap {text-align: center;padding-bottom: 25px;}
.quiz_wrap .scorewrap .score {color: #4792ff;font-size: 50px;font-weight: bold;margin: 40px 0 30px 0;}
.quiz_wrap .scorewrap .scoreTxt {color: var(--km-primary-40); font-size: 28px;font-weight: bold;line-height:1.6; padding: 40px 60px 10px; border-top: 1px solid var(--km-gray-sub-20); }
.quiz_wrap .scorewrap .scoreTxt02 { font-size: 18px; margin:20px 10px 40px;}
.quiz_wrap .resultQ .scorewrap .titR {font-size: 40px;line-height: 1.3;margin-bottom: 10px;text-align: center;font-weight: 600;color:#1FB58A;}
.quiz_wrap .resultQ .detailBox img { margin-top:50px;}
.quiz_wrap .resultQ .scorewrap .box {position: relative;margin-bottom: -5px;}
.quiz_wrap .resultQ .scorewrap .comment {font-size: 20px;line-height: 1.7;padding-top: 20px;text-align: left; margin: 30px 20px 20px;}
.quiz_wrap .resultQ .scorewrap .graph {margin-top: -15px;margin-bottom: 40px;}
.quiz_wrap .resultQ .scorewrap .graph .txt {margin-bottom: 12px;}
.quiz_wrap .resultQ .scorewrap .graph .num {color: #1cc3bb;font-weight: bold;font-size: 30px;}

/*뉴스레터*/
.km-newsletter-view * { margin:0px; padding:0px;}
.km-newsletter-view .cs-entry__title { font-size:28px;}

.km-newsletter-view-v2 *{ margin:0px; padding:0px;}
.km-newsletter-view-v2 h3 { margin:0px !important; padding:0px !important;}
.km-newsletter-view-v2 br { font-size:0px; line-height:0px; height:0px; max-height:0px; background:red;display: none;}
.km-newsletter-view-v2 { padding-top:20px; padding-bottom:20px; background:#F2F4F9; border-top:4px solid #2265E3 }
.km-newsletter-view-v2 table table table table table table { display: inline !important; text-align: right;} 
.post-btn-subscription { background:#1FB58A;}
.cs-entry__post-meta > .post-btn-subscription:not(:first-child):before{ display:none;}


/*--------------- List ---------------*/

/*post list*/
.cs-page__title:not(:first-child) { margin-top:40px; text-align:center;}
.cs-page__archive-count:not(:first-child), .cs-page__subtitle { display:none; }
.head-post-wrap { display:flex; gap:40px; flex-direction:row; margin-top:30px; margin-bottom:30px;}
.head-post-wrap .cs-entry:nth-child(n) { border:none;}
.head-post-wrap .cs-entry__thumbnail { max-width:100%; margin-right:0;}
.head-post-wrap .cs-entry__inner.cs-entry__overlay{ height:320px; }
.head-post-wrap .cs-entry:nth-child(n) { border:none !important; padding:0 !important;}
.head-post-wrap .cs-entry__excerpt { font-size:15px !important; color:var(--km-gray-txt-60) !important;}
.head-post-wrap .cs-entry__outer { display:initial;}
.head-post-wrap .cs-meta-author, .sub-post-wrap .cs-meta-author { display:none;}
.head-post-wrap .cs-entry__post-meta > *:before,.sub-post-wrap .cs-entry__post-meta > *:before{ content:""; margin-left: -4px;}
:where(.head-post-wrap,.sub-post-wrap) :where(.cs-meta-date) { color:var(--km-gray-txt-30);}
:where(.head-post-wrap,.sub-post-wrap) :where(.cs-overlay-background) { background:var(--wpr-bg-9e409632-ed3c-4f0c-819e-8b333e09e0c2) var(--km-gray-sub-10) center center no-repeat; background-size:20%}
:where(.head-01,.head-02) :where(.cs-entry__excerpt) {overflow: hidden; word-wrap: break-word; display: -webkit-box; -webkit-line-clamp: 3; -webkit-box-orient: vertical;}
.head-post-wrap .head-01 { width:50%; }
.head-post-wrap .head-01 .cs-entry:first-child .cs-entry__title, .head-post-wrap .head-02 .cs-entry:first-child:nth-last-child(1) .cs-entry__title { font-size:32px; font-weight:800 !important;}
.head-post-wrap .head-01 .cs-entry__inner:not(:first-child) { margin-top:20px;}
.head-post-wrap .head-02 { width:50%; display:flex; flex-direction:row; gap:16px; }
.head-post-wrap .head-02 .cs-entry:nth-child(n) { width:50%;}
.head-post-wrap .head-02 .cs-entry:first-child:nth-last-child(1) {width:100%;}
.head-post-wrap .head-02 .cs-entry__inner:not(:first-child) { margin-top:20px;}
.head-post-wrap .head-02 .cs-entry__title { font-size:24px; font-weight:800 !important; overflow: hidden; word-wrap: break-word; display: -webkit-box; -webkit-line-clamp: 2; -webkit-box-orient: vertical;}

.sub-post-wrap {border-top:3px solid var(--km-gray-txt-100); margin-top:40px;}
.sub-post-wrap .cs-entry:nth-child(n) { border:none !important; border-bottom:1px solid var(--km-gray-sub-40) !important; padding:24px 0 !important; }
.sub-post-wrap .cs-entry__thumbnail { max-width:220px;}
.sub-post-wrap .cs-entry__title { font-size:20px; padding-top:8px; }
.sub-post-wrap .cs-entry__excerpt { font-size:15px !important; color:var(--km-gray-txt-60) !important;}

/*author list*/
.km-authorList .wp-block-group__inner-container > * { margin-bottom:0px !important; margin-top:0px !important;}
:where(.km-authorList .is-layout-grid) { gap:16px;}
.km-authorList .author-description em { display:block; font-style:inherit; color:var(--km-gray-txt-80); font-weight:600; margin-bottom:4px; }
.km-authorList .author-description { text-align:left; overflow: hidden; word-wrap: break-word; display: -webkit-box; -webkit-line-clamp: 3; -webkit-box-orient: vertical;}
.km-authorList .pk-widget-author-container {  border:1px solid var(--km-gray-sub-30); background:var(--km-gray-sub-3); padding:32px 20px; min-height:180px; }
.km-authorList .pk-author-avatar img { max-width: 80px; background: var(--km-gray-sub-20); border: 1px solid var(--km-gray-sub-30);}
.km-authorList .km-author-list-con { text-align: left; margin-left:12px;}
.km-authorList .cs-main-content .entry-content .wp-block-group__inner-container > * { margin-top:0px; margin-bottom:0px;}
.km-authorList .pk-author-avatar {text-align: left; display: table-cell; width: 100px;}
.km-authorList .pk-widget-author .pk-author-title { margin-bottom: 1rem; float: inline-start; margin-left: 100px; position: absolute; display: table-cell;}
.km-authorList .pk-widget-author .pk-author-data { border-top: 1px solid var(--km-gray-sub-30); padding-top: 12px; display: table-cell; position: absolute; left: 120px; top: 50px; right:20px}  
.sub-post-wrap .td-post-category { display:none; }
.sub-post-wrap .cs-entry__title:not(:first-child) { margin-top:0px;}

.cs-main-content .entry-content > h2.km-sub-tit{ margin:36px 0 0 0; text-align:center;}

.category-list { display:flex; align-items: center; justify-content: center; margin-top:40px; flex-wrap:wrap; margin-left:-40px;}
.category-list li { background:var(--km-gray-sub-30); padding:8px 20px; margin-left:8px; margin-bottom:8px; font-size:16px; font-weight:700; transition:all 0.2s ease-out;}
.category-list li.on { background:var(--km-gray-sub-100); }
.category-list li:hover {background:var(--km-gray-sub-100);  color:var(--km-gray-txt-1);}
.category-list li.on a, .category-list li:hover a {color:var(--km-gray-txt-1) !important;}

/*뉴스레터*/
.km-newsletter-list { border-top:3px solid var(--km-gray-txt-100); padding-top:0px; }
.km-newsletter-list .cs-entry:nth-child(n) { border:none !important; border-bottom:1px solid var(--cs-color-contrast-200) !important; padding:28px 8px !important;}
.km-newsletter-list .cs-entry:nth-child(n):hover { background:var(--km-primary-20); transition:all 0.2s ease-out;}
.km-newsletter-list .cs-entry__thumbnail { display:none; }
.km-newsletter-list .cs-entry__outer { display:flex; flex-direction:row; }
.km-newsletter-list .cs-entry__post-meta { order:1; min-width:100px; font-size:16px; color:var(--km-gray-txt-40) !important;}
.km-newsletter-list .cs-entry__post-meta:after { content:"|"; color:var(--km-gray-sub-40); padding-left:12px;}
.km-newsletter-list .cs-entry__post-meta:not(:first-child) { margin-top:0 !important;}
.km-newsletter-list .cs-entry__wrapper { order:2;}
.km-newsletter-list .cs-entry__wrapper .cs-entry__title a { font-size:18px !important; font-weight:600;  overflow: hidden; word-wrap: break-word; display: -webkit-box; -webkit-line-clamp: 1; -webkit-box-orient: vertical; }

/*paging*/
.cs-posts-area__pagination { margin-bottom:10rem; }
.navigation.pagination .nav-links > a { font-size:14px; padding:3px 10px; min-width:auto; min-height:auto; color:var(--km-gray-txt-40); margin:0 4px; }
.navigation.pagination .nav-links > span { padding:3px 10px; min-width:auto; min-height:auto;}
.navigation.pagination .nav-links > span.dots { padding:0px; color:#ccc; }
.navigation.pagination .nav-links > .current { background-color:var(--km-primary-40); color:#fff; font-size:14px;}
.navigation.pagination .nav-links a:first-child, .navigation.pagination .nav-links a:last-child { }
.navigation.pagination .nav-links a:first-child, .navigation.pagination .nav-links a:last-child {border:1px solid var(--km-gray-sub-30); background:var(--km-primary-10); border-radius:40px; font-size:0px; }
navigation.pagination .nav-links a:first-child { margin-right:12px;}
.navigation.pagination .nav-links a:last-child {margin-left:12px;}
.navigation.pagination .nav-links a.page-numbers:hover { background:var(--km-gray-sub-40);  border-radius:40px; }
.navigation.pagination .nav-links a:last-child:after { margin-left:0; font-size:13px; }
.navigation.pagination .nav-links a:first-child:before { margin-right:0; font-size:13px;}

/*팟캐스트player*/
.post_podcast {width:100%}
.post_podcast .mejs-container { background:var(--km-gray-txt-1) !important;}
.post_podcast .mejs-container .mejs-controls{ background:var(--km-primary-20) !important;border-radius:4px;}
.post_podcast 
  .mejs-time-buffering,
  .mejs-time-current,
  .mejs-time-float,
  .mejs-time-float-corner,
  .mejs-time-float-current,
